<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luxoft.sbrf.iask.persistence.common.dao.AbstractDao">

    <select id="fixUser" statementType="CALLABLE" parameterType="string" resultType="string">
        { CALL qchSetUserFixed(#{login}) }
    </select>

    <select id="noCountOn" statementType="STATEMENT">
        SET NOCOUNT ON
    </select>

    <select id="noCountOff" statementType="STATEMENT">
        SET NOCOUNT OFF
    </select>

    <select id="setConcatNullYieldsNullOn" statementType="STATEMENT">
        SET CONCAT_NULL_YIELDS_NULL ON
    </select>

    <select id="setConcatNullYieldsNullOff" statementType="STATEMENT">
        SET CONCAT_NULL_YIELDS_NULL OFF
    </select>

</mapper>